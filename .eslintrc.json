{
  "env": {
    "browser": true,
    "cypress/globals": true
  },
  "plugins": [
    "html",
    "standard",
    "vue",
    "cypress"
  ],
  "extends": [
    "standard",
    "plugin:vue/recommended",
    "plugin:cypress/recommended"
  ],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "vue/singleline-html-element-content-newline": "off"
  }
}
